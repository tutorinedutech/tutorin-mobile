<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/tv_desc_register_1"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="10dp"
            android:scaleX="1"
            android:scaleY="1"
            android:translationY="0dp" />
        <Constraint
            android:id="@+id/btn_view_register_learner"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:rotation="-90"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:scaleX="1"
            android:scaleY="1"
            android:translationY="0dp"
            android:layout_marginTop="20dp" />
        <Constraint
            android:id="@+id/btn_view_register_tutor"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="40dp"
            android:layout_height="40dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:rotation="90"
            motion:layout_constraintStart_toStartOf="parent"
            android:scaleX="1"
            android:scaleY="1"
            android:translationY="0dp"
            android:layout_marginBottom="20dp" />
        <Constraint
            android:id="@+id/textView"
            motion:layout_constraintEnd_toEndOf="@id/btn_view_register_learner"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:alpha="1"
            motion:layout_constraintStart_toStartOf="@id/btn_view_register_learner"
            android:scaleX="1"
            android:scaleY="1"
            android:translationY="0dp"
            motion:layout_constraintTop_toBottomOf="@id/btn_view_register_learner" />
        <Constraint
            android:id="@+id/textView2"
            motion:layout_constraintEnd_toEndOf="@id/btn_view_register_tutor"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toTopOf="@id/btn_view_register_tutor"
            android:alpha="1"
            motion:layout_constraintStart_toStartOf="@id/btn_view_register_tutor"
            android:scaleX="1"
            android:scaleY="1"
            android:translationY="0dp" />
        <Constraint
            android:id="@+id/tv_header_register"
            motion:layout_constraintEnd_toEndOf="@id/tv_desc_register_1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toTopOf="@id/tv_desc_register_1"
            android:alpha="1"
            motion:layout_constraintStart_toStartOf="@id/tv_desc_register_1"
            android:scaleX="1"
            android:scaleY="1"
            android:translationY="0dp" />
        <Constraint
            android:id="@+id/tv_desc_register_2"
            motion:layout_constraintEnd_toEndOf="@id/tv_desc_register_1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/tv_desc_register_1"
            android:alpha="1"
            motion:layout_constraintStart_toStartOf="@+id/tv_desc_register_1"
            android:layout_marginTop="10dp"
            android:scaleX="1"
            android:scaleY="1"
            android:translationY="0dp" />
        <Constraint
            android:id="@+id/sv_learner"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            motion:layout_constraintBottom_toTopOf="parent"
            motion:layout_editor_absoluteX="20dp" />
        <Constraint
            android:id="@+id/sv_tutor"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            motion:layout_constraintTop_toBottomOf="parent" />
    </ConstraintSet>

    <ConstraintSet
        android:id="@+id/register_tentor"
        motion:deriveConstraintsFrom="@+id/start" >
        <Constraint
            android:id="@+id/btn_view_register_learner"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:alpha="0"
            android:rotation="-90"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:scaleX="0.9"
            android:scaleY="0.9"
            android:translationY="-50dp" />
        <Constraint
            android:id="@+id/textView"
            motion:layout_constraintEnd_toEndOf="@id/btn_view_register_learner"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@id/btn_view_register_learner"
            android:alpha="0"
            motion:layout_constraintStart_toStartOf="@id/btn_view_register_learner"
            android:scaleX="0.9"
            android:scaleY="0.9"
            android:translationY="-50dp" />
        <Constraint
            android:id="@+id/textView2"
            motion:layout_constraintEnd_toEndOf="@id/btn_view_register_tutor"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toTopOf="@id/btn_view_register_tutor"
            android:alpha="0"
            motion:layout_constraintStart_toStartOf="@id/btn_view_register_tutor"
            android:scaleX="0.9"
            android:scaleY="0.9"
            android:translationY="-50dp" />
        <Constraint
            android:id="@+id/btn_view_register_tutor"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="40dp"
            android:layout_height="40dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:alpha="0"
            android:rotation="90"
            motion:layout_constraintStart_toStartOf="parent"
            android:scaleX="0.9"
            android:scaleY="0.9"
            android:translationY="-50dp" />
        <Constraint
            android:id="@+id/tv_header_register"
            motion:layout_constraintEnd_toEndOf="@id/tv_desc_register_1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toTopOf="@id/tv_desc_register_1"
            android:alpha="0"
            motion:layout_constraintStart_toStartOf="@id/tv_desc_register_1"
            android:scaleX="0.9"
            android:scaleY="0.9"
            android:translationY="-50dp" />
        <Constraint
            android:id="@+id/tv_desc_register_1"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:alpha="0"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="10dp"
            android:scaleX="0.9"
            android:scaleY="0.9"
            android:translationY="-50dp" />
        <Constraint
            android:id="@+id/tv_desc_register_2"
            motion:layout_constraintEnd_toEndOf="@id/tv_desc_register_1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/tv_desc_register_1"
            android:alpha="0"
            motion:layout_constraintStart_toStartOf="@+id/tv_desc_register_1"
            android:layout_marginTop="10dp"
            android:scaleX="0.9"
            android:scaleY="0.9"
            android:translationY="-50dp" />
        <Constraint
            android:id="@+id/sv_tutor"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent" />
    </ConstraintSet>
    <ConstraintSet
        android:id="@+id/register_learner"
        motion:deriveConstraintsFrom="@+id/start" >
        <Constraint
            android:id="@+id/btn_view_register_learner"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:rotation="-90"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="20dp"
            android:alpha="0"
            android:scaleX="0.9"
            android:scaleY="0.9"
            android:translationY="50dp" />
        <Constraint
            android:id="@+id/textView"
            motion:layout_constraintEnd_toEndOf="@id/btn_view_register_learner"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@id/btn_view_register_learner"
            android:alpha="0"
            motion:layout_constraintStart_toStartOf="@id/btn_view_register_learner"
            android:scaleX="0.9"
            android:scaleY="0.9"
            android:translationY="50dp" />
        <Constraint
            android:id="@+id/textView2"
            motion:layout_constraintEnd_toEndOf="@id/btn_view_register_tutor"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toTopOf="@id/btn_view_register_tutor"
            android:alpha="0"
            motion:layout_constraintStart_toStartOf="@id/btn_view_register_tutor"
            android:scaleX="0.9"
            android:scaleY="0.9"
            android:translationY="50dp" />
        <Constraint
            android:id="@+id/btn_view_register_tutor"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:rotation="90"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="20dp"
            android:alpha="0"
            android:scaleX="0.9"
            android:scaleY="0.9"
            android:translationY="50dp" />
        <Constraint
            android:id="@+id/tv_header_register"
            motion:layout_constraintEnd_toEndOf="@id/tv_desc_register_1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toTopOf="@id/tv_desc_register_1"
            android:alpha="0"
            motion:layout_constraintStart_toStartOf="@id/tv_desc_register_1"
            android:scaleX="0.9"
            android:scaleY="0.9"
            android:translationY="50dp" />
        <Constraint
            android:id="@+id/tv_desc_register_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:alpha="0"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="10dp"
            android:scaleX="0.9"
            android:scaleY="0.9"
            android:translationY="50dp" />
        <Constraint
            android:id="@+id/tv_desc_register_2"
            motion:layout_constraintEnd_toEndOf="@id/tv_desc_register_1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/tv_desc_register_1"
            android:alpha="0"
            motion:layout_constraintStart_toStartOf="@+id/tv_desc_register_1"
            android:layout_marginTop="10dp"
            android:scaleX="0.9"
            android:scaleY="0.9"
            android:translationY="50dp" />
        <Constraint
            android:id="@+id/sv_learner"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent" />
    </ConstraintSet>
    <Transition
        motion:constraintSetStart="@+id/start"
        motion:constraintSetEnd="@+id/register_tentor" >
        <OnSwipe motion:dragDirection="dragUp" />
        <OnClick motion:targetId="@id/btn_view_register_tutor" />
    </Transition>
    <Transition
        motion:constraintSetStart="@+id/start"
        motion:constraintSetEnd="@+id/register_learner">
        <OnSwipe motion:dragDirection="dragDown" />
        <OnClick motion:targetId="@id/btn_view_register_learner" />
    </Transition>
    <Transition
        motion:constraintSetStart="@+id/register_tentor"
        motion:constraintSetEnd="@+id/start" >
        <OnSwipe motion:dragDirection="dragDown" />
    </Transition>
    <Transition
        motion:constraintSetStart="@+id/register_learner"
        motion:constraintSetEnd="@+id/start" >
        <OnSwipe motion:dragDirection="dragUp" />
    </Transition>
</MotionScene>